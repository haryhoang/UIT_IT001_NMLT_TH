#include <iostream>
#include <string.h>
using namespace std;
#define MAX 100
void input(char s[])
{
    cin.getline(s, MAX);
}

void clearstring(char s[])
{
    int len = strlen(s);
    int stat = 0;
    while (s[stat] == ' ' && stat < len)
    {
        stat ++;
    }

    int endd = len-1;
    while (s[endd] == ' ' && endd >= stat)
        endd--;

    int k;
    char s1[MAX];
    for (int i=stat; i<= endd; i++)
    {
        s1[k++] = s[i];
    }
    s1[k] = '\0';
    strcpy(s, s1);
}


void output(char s[])
{
    int len = strlen(s);
    if ( len == 0 )
    {
        cout << "Chuoi rong.";
        return ;
    }
    clearstring(s);
    char cop[MAX];
    strcpy(cop, s);
    len = strlen(s);
    int stat = 0;
    for (int i=0; i<len; i++)
    {
        if (s[i+1] == ' ' || s[i+1] == '\0')
        {
            int backk = i;
            for (int j = stat; j<=i; j++)
            {
                cop[j] = s[backk--];
            }
            stat = i + 2;
        }
    }
    strcpy(s, cop);
    cout << s;

    return ;
}

int main()
{
    char s[MAX];
    input(s);
    output(s);
}
